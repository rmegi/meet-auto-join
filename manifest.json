{
  "manifest_version": 3,
  "name": "Google Meet Quick Joiner",
  "version": "1.1",
  "description": "Quickly join a Google Meet using a 6-digit code.",
  "permissions": ["tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Join Meet"
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": ["https://meet.google.com/*"]
}
